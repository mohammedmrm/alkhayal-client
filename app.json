{
  "expo": {
    "name": "جوهرة الشمال - العميل",
    "slug": "alamenClient",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./app/assets/logo/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./app/assets/logo/logo.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/n1.wav",
            "./assets/n2.wav"
          ]
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alamen.client",
      "buildNumber": "1.0.2"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/logo/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "package": "com.alamen.client",
      "versionCode": 3
    },
    "web": {
      "favicon": "./app/assets/logo/logo.png"
    },
    "notification": {
      "icon": "./app/assets/logo/logo.png",
      "color": "#FFFFFF",
      "iosDisplayInForeground": true,
      "androidMode": "default"
    },
    "extra": {
      "eas": {
        "projectId": "c79d5808-469c-4a83-a6e7-9b832212899f"
      }
    }
  }
}
