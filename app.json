{
  "expo": {
    "name": "شركة سرعه  - العميل",
    "slug": "speedcomClient",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/logo/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./app/assets/logo/logo.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/n1.wav",
            "./assets/n2.wav"
          ]
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.speedcom.client",
      "buildNumber": "1.0.3"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/logo/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.speedcom.client",
      "versionCode": 2
    },
    "web": {
      "favicon": "./app/assets/logo/logo.png"
    },
    "notification": {
      "icon": "./app/assets/logo/logo.png",
      "color": "#FFFFFF",
      "iosDisplayInForeground": true,
      "androidMode": "default"
    },
    "extra": {
      "eas": {
        "projectId": "a601b8bd-bdd0-41c9-8367-558b91c88fd7"
      }
    }
  }
}