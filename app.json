{
  "expo": {
    "name": "شركة السنجري - العميل",
    "slug": "alsanjariClient",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/logo/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./app/assets/logo/logo.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/n1.wav",
            "./assets/n2.wav"
          ]
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alsanjarl.client",
      "buildNumber": "1.0.1"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/logo/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.alsanjar.client",
      "versionCode": 4
    },
    "web": {
      "favicon": "./app/assets/logo/logo.png"
    },
    "notification": {
      "icon": "./app/assets/logo/logo.png",
      "color": "#FFFFFF",
      "iosDisplayInForeground": true,
      "androidMode": "default"
    }
  }
}